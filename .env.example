# Rune Environment Variables
# Copy this file to .env and fill in your actual values

# Telemetry Configuration
## Toggle telemetry entirely (set to "true" to disable)
RUNE_TELEMETRY_DISABLED=false

## Enable verbose debug logging
RUNE_DEBUG=false

## Deployment environment (development|staging|production)
RUNE_ENV=production

## OpenTelemetry OTLP HTTP endpoint for logs
# Examples:
#   http://localhost:4318/v1/logs (local Collector)
#   https://otel-collector.example.com/v1/logs (managed Collector)
RUNE_OTLP_ENDPOINT=

## Sentry DSN - get this from your Sentry project
## Used for error tracking and performance monitoring
## Format: https://public_key@oXXXXXXX.ingest.sentry.io/project_id
RUNE_SENTRY_DSN=
