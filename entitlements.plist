<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- Allow network access for telemetry and integrations -->
    <key>com.apple.security.network.client</key>
    <true/>
    
    <!-- Allow network server capabilities if needed -->
    <key>com.apple.security.network.server</key>
    <true/>
    
    <!-- Allow file access for configuration and data storage -->
    <key>com.apple.security.files.user-selected.read-write</key>
    <true/>
    
    <!-- Allow access to user's home directory for config files -->
    <key>com.apple.security.files.user-selected.read-only</key>
    <true/>
    
    <!-- Allow access to user's Downloads folder for potential file operations -->
    <key>com.apple.security.files.downloads.read-write</key>
    <true/>
    
    <!-- Runtime hardening -->
    <key>com.apple.security.cs.allow-jit</key>
    <false/>
    
    <key>com.apple.security.cs.allow-unsigned-executable-memory</key>
    <false/>
    
    <key>com.apple.security.cs.disable-executable-page-protection</key>
    <false/>
    
    <key>com.apple.security.cs.disable-library-validation</key>
    <false/>
    
    <!-- Allow dyld environment variables for Go runtime -->
    <key>com.apple.security.cs.allow-dyld-environment-variables</key>
    <false/>
</dict>
</plist>